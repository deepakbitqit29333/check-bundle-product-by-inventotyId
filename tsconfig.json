{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES6",
    "lib": [
      "esnext",
      "dom"
    ],
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
  },
  "include": [
    "src/Constants.ts",
    "src/index.ts"
  ]
}